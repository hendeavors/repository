{
    "name": "endeavors/repository",
    "type": "library",
    "description": "A simple repository pattern in php",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "^5.5 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Endeavors\\Repository\\": "src/Repository"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endeavors\\Repository\\Tests\\": "tests/"
        }
    }
}
